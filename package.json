{
  "name": "duty",
  "version": "1.0.0",
  "private": true,
  "description": "application for generating guard duty names",
  "main": "index.js",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "env-cmd -f ./.env nodemon ./bin/www",
    "test": "jest --watch"
  },
  "author": "Shawn Loh",
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/src/controllers/__tests__/utils/"
    ],
    "setupFiles": [
      "<rootDir>/tests/setupEnv.js"
    ]
  },
  "dependencies": {
    "bcrypt": "^3.0.7",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "connect-timeout": "^1.9.0",
    "express": "^4.17.1",
    "express-validator": "^6.3.0",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.27",
    "mongoose": "^5.7.14",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.24",
    "env-cmd": "^10.0.1",
    "eslint": "^6.7.2",
    "eslint-config-google": "^0.14.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2"
  }
}
